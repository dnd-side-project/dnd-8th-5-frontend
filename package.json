{
  "name": "dnd-8th-5-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite --host 0.0.0.0 --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "deploy-storybook": "storybook-to-ghpages -d -o=storybook-static --ci",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --config ./.prettierrc --write -u"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@sentry/vite-plugin": "^3.2.2",
    "@storybook/storybook-deployer": "^2.8.16",
    "@tanstack/react-query": "^4.24.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.4.9",
    "@types/react": "^18.2.0",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.2.1",
    "@types/react-slick": "^0.23.10",
    "@types/slick-carousel": "^1.6.37",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-calendar": "4.0.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-multi-date-picker": "3.3.4",
    "react-router-dom": "^6.8.1",
    "react-scripts": "5.0.1",
    "react-scrollable-picker": "^1.0.2",
    "react-selecto": "^1.26.0",
    "react-slick": "^0.29.0",
    "recoil": "^0.7.6",
    "slick-carousel": "^1.8.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@sentry/react": "^9.9.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@tanstack/react-query-devtools": "^4.24.4",
    "@types/react-calendar": "3.9.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^4.0.4",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
